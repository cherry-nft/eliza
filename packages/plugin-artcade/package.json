{
    "name": "@ai16z/plugin-artcade",
    "version": "0.1.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "dependencies": {
        "@ai16z/eliza": "workspace:*",
        "@ai16z/adapter-postgres": "workspace:*",
        "jsdom": "^22.1.0",
        "uuid": "^9.0.1",
        "express": "^4.18.2",
        "pg": "^8.11.3"
    },
    "devDependencies": {
        "typescript": "^5.0.0",
        "tsup": "^8.0.0",
        "vitest": "^0.34.0",
        "@types/node": "^20.0.0",
        "@types/jsdom": "^21.1.6",
        "@types/uuid": "^9.0.7",
        "@types/express": "^4.17.21",
        "jsdom": "^22.1.0",
        "ts-node": "^10.9.2"
    },
    "scripts": {
        "build": "tsup src/index.ts --format esm,cjs --dts",
        "test": "node --max-old-space-size=4096 node_modules/vitest/vitest.mjs run --environment jsdom",
        "dev": "tsup src/index.ts --format esm,cjs --watch",
        "viewer": "NODE_OPTIONS=\"--loader ts-node/esm\" node test-harness/server.ts",
        "viewer:dev": "nodemon -r ts-node/register test-harness/server.ts",
        "test:evolution": "node --loader ts-node/esm playground/src/test-evolution.ts"
    }
}
