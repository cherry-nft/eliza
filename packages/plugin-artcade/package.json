    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "dependencies": {
        "@ai16z/adapter-postgres": "workspace:*",
        "@ai16z/eliza": "workspace:*",
        "@supabase/supabase-js": "2.46.2",
        "express": "^4.18.2",
        "jsdom": "^22.1.0",
        "openai": "4.73.0",
        "pg": "^8.13.1",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
